{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@env/*": ["./build/src/env/prod/*"],
      "~/*": ["./build/src/*"]
    },
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "sourceMap": true,
    "strict": false,
    "outDir": "./build/src",
    "noEmitOnError": true,
    "typeRoots": ["./build/src/types", "./node_modules/@types"]
  },
  "include": ["build/src"]
}
