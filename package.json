{
  "name": "git-log-ui",
  "version": "0.1.0",
  "private": false,
  "description": "The tool generates git calendar logs depending on teams' activities",
  "author": "Maxim Karpov <makame@mail.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/makamekm/git-log-calendar-teams-ui.git"
  },
  "keywords": [
    "git",
    "log",
    "calendar",
    "teams",
    "cli",
    "ui"
  ],
  "productName": "Git Log Calendar Teams UI",
  "main": "src/electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "run-p start start-electron",
    "start": "export BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && wait-for-localhost 3000 && electron --remote-debugging-port=9223 .",
    "start-electron:only": "export ELECTRON_START_URL=http://localhost:3000 && electron --remote-debugging-port=9223 .",
    "build": "react-scripts build",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "build-electron:windows": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package": "electron-builder build --mac --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/debounce": "^1.2.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "debounce": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-reloader": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "wait-for-localhost-cli": "^1.1.0"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "git-log-calendar-teams-ui",
      "owner": "makamekm"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
