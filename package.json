{
  "name": "git-log-ui",
  "version": "0.1.0",
  "private": false,
  "description": "The tool generates git calendar logs depending on teams' activities",
  "author": "Maxim Karpov <makame@mail.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/makamekm/git-log-calendar-teams-ui.git"
  },
  "keywords": [
    "git",
    "log",
    "calendar",
    "teams",
    "cli",
    "ui"
  ],
  "productName": "Git Log Manager",
  "main": "src/electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "run-p start start-electron",
    "start": "export BROWSER=none && react-app-rewired start",
    "dev:electron": "nodemon --watch src/electron --watch src/shared -e js,ts,tsx --exec \"npm run start-electron\"",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && export NODE_ENV=development && wait-for-localhost 3000 && electron --remote-debugging-port=9223 .",
    "start-electron:only": "export ELECTRON_START_URL=http://localhost:3000 && electron --remote-debugging-port=9223 .",
    "build": "react-app-rewired build",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "build-electron:windows": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package": "electron-builder build --mac --win -c.extraMetadata.main=build/electron/main.js --publish never --icon=public/icon.win.ico",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@nivo/calendar": "^0.61.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "@types/classnames": "^2.2.10",
    "@types/debounce": "^1.2.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/node-sass": "^4.11.0",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-grid-layout": "^0.17.1",
    "@types/react-helmet": "^5.0.15",
    "@types/react-image-crop": "^8.1.2",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-toggle": "^4.0.2",
    "@types/reactstrap": "^8.4.2",
    "@types/text-mask-addons": "^3.8.0",
    "@types/uuid": "^7.0.2",
    "ag-grid-community": "^23.0.2",
    "ag-grid-react": "^23.0.3",
    "animejs": "^3.1.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "customize-cra": "^0.9.1",
    "debounce": "^1.2.0",
    "faker": "^4.1.0",
    "font-awesome": "^4.7.0",
    "git-log-calendar-teams": "^1.0.6",
    "holderjs": "^2.9.6",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.0",
    "rc-slider": "^9.2.4",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-beautiful-dnd": "^13.0.0",
    "react-big-calendar": "^0.24.1",
    "react-bootstrap-table-next": "^4.0.0",
    "react-bootstrap-table2-editor": "^1.4.0",
    "react-bootstrap-table2-filter": "^1.3.1",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.2",
    "react-bootstrap-typeahead": "^4.1.0",
    "react-content-loader": "^5.0.4",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-grid-layout": "^0.18.3",
    "react-helmet": "^5.2.1",
    "react-image-crop": "^8.6.1",
    "react-quill": "^1.3.5",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^5.5.0",
    "react-toggle": "^4.1.1",
    "reactstrap": "^8.4.1",
    "recharts": "^2.0.0-beta.5",
    "text-mask-addons": "^3.8.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "wait-for-localhost-cli": "^1.1.0"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "git-log-calendar-teams-ui",
      "owner": "makamekm"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
